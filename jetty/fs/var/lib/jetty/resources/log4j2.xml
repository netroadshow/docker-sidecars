<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="Jetty">
  <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <JsonLayout complete="false" compact="true" eventEol="true" stacktraceAsString="true">
        <KeyValuePair key="time" value="$${date:yyyy-MM-dd'T'HH:mm:ss.SSS}" />
      </JsonLayout>
    </Console>
  </Appenders>

  <Loggers>
    <Root level="info">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>